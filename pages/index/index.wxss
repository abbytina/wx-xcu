/**index.wxss**/

.swiper swiper, .swiper image {
    width: 750rpx;
    height: 390rpx;
}

.pic-nav {
    display: flex;
    flex-wrap: wrap;
}

.pic-nav .item {
    display: flex;
    width: 33.3%;
    height: 250rpx;
    box-sizing: border-box;
    flex-direction: column;
    justify-content: center;
    align-items: center;
    border-bottom:1rpx solid #eee;
}

.pic-nav .item image {
    width: 80rpx;
    height: 90rpx;
}

.pic-nav .item text {
    font-size: 28rpx;
}
.row{
    padding: 30rpx 20rpx;
}
.row .col{
    display: flex;
    justify-content: center;
    align-items: center;
}
.row .col image{
    width: 300rpx;
    height: 200rpx;
}
